{
  "name": "qash-server",
  "version": "0.0.2",
  "description": "server base app",
  "author": "Quantum3 Labs <contact@quantum3labs.com>",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" && npx prisma format --schema ./src/database/prisma/schema.prisma --config ./src/database/prisma/prisma.config.ts",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:openHandles": "jest --detectOpenHandles",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "pnpm run docker:db:test:up && jest --config ./test/jest-e2e.json --forceExit && pnpm run docker:db:test:down",
    "test:e2e:ps": "powershell -File ./test/helpers/run-e2e-test.ps1",
    "docker:db:up": "docker compose --env-file .env up -d && pnpm run build && sleep 5 && prisma migrate deploy --schema ./src/database/prisma/schema.prisma --config ./src/database/prisma/prisma.config.ts",
    "docker:db:down": "docker compose --env-file .env down -v",
    "docker:db:test:up": "docker compose --env-file .env.test up -d && pnpm run build && sleep 5 && pnpm run migration:run",
    "docker:db:test:down": "docker compose --env-file .env.test down -v",
    "prisma:generate": "prisma generate --schema ./src/database/prisma/schema.prisma --config ./src/database/prisma/prisma.config.ts",
    "prisma:reset": "prisma migrate reset --schema ./src/database/prisma/schema.prisma --config ./src/database/prisma/prisma.config.ts",
    "prisma:migrate:deploy": "prisma migrate deploy --schema ./src/database/prisma/schema.prisma --config ./src/database/prisma/prisma.config.ts",
    "prisma:migrate:dev": "prisma migrate dev --schema ./src/database/prisma/schema.prisma --config ./src/database/prisma/prisma.config.ts",
    "prisma:studio": "npx prisma studio --config ./src/database/prisma/prisma.config.ts",
    "puppeteer:install": "npx puppeteer browsers install chrome"
  },
  "dependencies": {
    "@json2csv/plainjs": "^7.0.6",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/common": "^11.1.9",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.9",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.9",
    "@nestjs/platform-socket.io": "^11.1.9",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/swagger": "^11.2.3",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/typeorm": "^11.0.0",
    "@nestjs/websockets": "^11.1.9",
    "@prisma/adapter-pg": "^7.0.1",
    "@prisma/client": "^7.0.1",
    "@prisma/client-runtime-utils": "^7.0.1",
    "@types/ws": "^8.18.1",
    "archiver": "^7.0.1",
    "bcrypt": "^6.0.0",
    "body-parser": "^2.2.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "express-session": "^1.18.2",
    "helmet": "^8.1.0",
    "joi": "^18.0.2",
    "json2csv": "^6.0.0-alpha.2",
    "nanoid": "^5.1.6",
    "nestjs-mailgun": "^4.0.3",
    "otplib": "^12.0.1",
    "papaparse": "^5.5.3",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "puppeteer": "^24.33.1",
    "qrcode": "^1.5.4",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "socket.io": "^4.8.1",
    "swagger-themes": "^1.4.3",
    "typeorm": "^0.3.27",
    "ws": "^8.18.3",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@golevelup/ts-jest": "^0.7.0",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/cli": "^11.0.14",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.9",
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "@types/nodemailer": "^7.0.4",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^30.2.0",
    "prettier": "^3.7.3",
    "prisma": "^7.0.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/$1"
    }
  }
}
